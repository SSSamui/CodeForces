#include <iostream>
#include <cstdio>
#include <vector>
#include <cmath>
using namespace std;
using ll = long long;
 
const ll MOD = 998244353;
 
int main()
{
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
 
	int n, m;
	cin >> n >> m;
 
	vector<ll> last1(m + 1, 0), curr1(m + 1, 0);
	last1[0] = 1;
	for (int i = 0; i < m; i++)
	{
		for (int j = 0; j < m; j++) curr1[j + 1] = last1[j];
		for (int j = 0; j < m; j++) curr1[j] = (curr1[j] + last1[j + 1]) % MOD;
		last1 = curr1;
		curr1 = vector<ll>(m + 1, 0);
	}
 
	vector<ll> last2 = last1, curr2(m + 1, 0);
	for (int i = 1; i < n; i++)
	{
		for (int j = 0; j <= m; j += 2) for (int k = 0; k <= j; k += 2)
			curr2[j - k] = (curr2[j - k] + last2[j] * last1[k]) % MOD;
		last2 = curr2;
		curr2 = vector<ll>(m + 1, 0);
	}
 
	cout << last2[0];
}
